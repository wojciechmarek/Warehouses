<Window x:Class="WarehousesFrontend.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WarehousesFrontend"
        xmlns:navs="clr-namespace:WarehousesFrontend.Views.Shared"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800" MinHeight="300" MinWidth="600"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="80"/>
        </Grid.RowDefinitions>

        <Frame Grid.Row="0" Background="Aqua">
            
        </Frame>

        <Frame Grid.Row="1" Source="/Views/Shared/NavigationButtons.xaml" NavigationUIVisibility="Hidden">
        </Frame>
    </Grid>
</Window>
